\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

\f1\b\fs36 \cf0 2. Accessing Elements\
\pard\pardeftab720\partightenfactor0

\f3\b0\fs26 \cf0 \
fruits = ["apple", "banana", "cherry"]\
print(fruits[0])   # apple\
print(fruits[-1])  # cherry (negative index = from last)\

\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

\f1\b\fs36 \cf0 3. Updating Elements\
\pard\pardeftab720\partightenfactor0

\f3\b0\fs26 \cf0 \
fruits = ["apple", "banana", "cherry"]\
fruits[1] = "mango"\
print(fruits)  # ['apple', 'mango', 'cherry']\

\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

\f1\b\fs36 \cf0 4. Adding Elements\
\pard\pardeftab720\partightenfactor0

\f3\b0\fs26 \cf0 \
fruits = ["apple", "banana"]\
\
# append() -> adds at the end\
fruits.append("cherry")\
print(fruits)  # ['apple', 'banana', 'cherry']\
\
# insert(index, value) -> adds at specific position\
fruits.insert(1, "mango")\
print(fruits)  # ['apple', 'mango', 'banana', 'cherry']\
\
# extend() -> adds multiple elements\
fruits.extend(["grapes", "orange"])\
print(fruits)  # ['apple', 'mango', 'banana', 'cherry', 'grapes', 'orange']\

\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

\f1\b\fs36 \cf0 5. Removing Elements\
\pard\pardeftab720\partightenfactor0

\f3\b0\fs26 \cf0 \
fruits = ["apple", "banana", "cherry", "mango"]\
\
# remove(value) -> removes first occurrence\
fruits.remove("banana")\
print(fruits)  # ['apple', 'cherry', 'mango']\
\
# pop(index) -> removes by index (default last)\
fruits.pop(1)\
print(fruits)  # ['apple', 'mango']\
\
# del -> delete element or whole list\
del fruits[0]\
print(fruits)  # ['mango']\
\
# clear() -> removes all elements\
fruits.clear()\
print(fruits)  # []\

\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

\f1\b\fs36 \cf0 6. Searching / Checking Elements\
\pard\pardeftab720\partightenfactor0

\f3\b0\fs26 \cf0 \
numbers = [10, 20, 30, 40]\
print(20 in numbers)   # True\
print(50 not in numbers)  # True\
print(numbers.index(30))  # 2\
print(numbers.count(10))  # 1\

\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

\f1\b\fs36 \cf0 7. List Slicing\
\pard\pardeftab720\partightenfactor0

\f3\b0\fs26 \cf0 \
nums = [0, 1, 2, 3, 4, 5, 6]\
\
print(nums[1:4])   # [1, 2, 3] (start=1, stop=4-1)\
print(nums[:3])    # [0, 1, 2]\
print(nums[4:])    # [4, 5, 6]\
print(nums[::2])   # [0, 2, 4, 6] (step=2)\
print(nums[::-1])  # [6, 5, 4, 3, 2, 1, 0] (reverse list)\

\f2\fs24 \cf2 \
\pard\pardeftab720\sa298\partightenfactor0

